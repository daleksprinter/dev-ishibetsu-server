version: '3'
services:
 app:
  build: './src'
  ports: 
   - '13334:13334'
  volumes: 
   - './src:/server'
  command: python3 db.py
  depends_on: db

 
 db:
  image: mysql:5.7
  container_name: 'db'
  ports:
   - "3306:3306"
  volumes:
   - ./mysql_data:/var/lib/mysql
   - ./db:/dumpfile
   - ./db/conf.d:/etc/mysql/conf.d
  environment:
   MYSQL_ROOT_PASSWORD: root
   MYSQL_USER: test
   MYSQL_PASSWORD: test
   MYSQL_DATABASE: test1492_testtable
  





